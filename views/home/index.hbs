<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/basscss@9.0.0-0/basscss.min.css">
        <title></title>
    </head>
    <body>
        <div>
            
        </div>
        <div class="container fluid p2">
        
        <table class="ui celled table">
          <thead>
            <tr>
                <th>SN</th>
                <th>KindSN</th>
                <th>ChapterNumber</th>
                <th>NewOrOld</th>
                <th>PinYin</th>
                <th>ShortName</th>
                <th>FullName</th>
            </tr>
          </thead>
          <tbody>
            {{#each bookId}}
                <tr>
                  <td data-label="SN">{{this.SN}}</td>
                  <td data-label="KindSN">{{this.KindSN}}</td>
                  <td data-label="ChapterNumber">{{this.ChapterNumber}}</td>
                  <td data-label="NewOrOld">{{this.NewOrOld}}</td>
                  <td data-label="PinYin">{{this.PinYin}}</td>
                  <td data-label="ShortName">{{this.ShortName}}</td>
                  <td data-label="FullName">{{this.FullName}}</td>
                </tr>
            {{/each}}
          </tbody>
        </table>
        </div>
    </body>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>
    <script src="script/home/index.js"></script>
</html>
